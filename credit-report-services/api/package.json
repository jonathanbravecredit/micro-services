{
  "name": "credit-report-api",
  "version": "0.1.0",
  "description": "A TypeScript starter for Serverless Framework",
  "main": "handler.js",
  "scripts": {
    "test": "serverless-bundle test",
    "testWatch": "serverless-bundle test --watchAll --verbose",
    "testCoverage": "serverless-bundle test --coverage",
    "testOpenHandles": "serverless-bundle test --detectOpenHandles"
  },
  "jest": {
    "moduleNameMapper": {
      "libs(.*)$": "<rootDir>/libs/$1",
      "src(.*)$": "<rootDir>/src/$1",
      "tests(.*)$": "<rootDir>/tests/$1"
    }
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AnomalyInnovations/serverless-typescript-starter.git"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.64",
    "@types/jest": "^26.0.14",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "serverless-bundle": "^5.2.0",
    "serverless-dotenv-plugin": "^3.10.0",
    "serverless-offline": "^8.3.1"
  },
  "dependencies": {
    "@bravecredit/brave-sdk": "^1.0.2",
    "@shiftcoders/dynamo-easy": "^7.1.0",
    "ajv": "^7.0.2",
    "dayjs": "^1.10.7",
    "reflect-metadata": "^0.1.13"
  }
}
